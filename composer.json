{
  "require": {
    "craftcms/cms": "3.0.36",
    "vlucas/phpdotenv": "^2.4.0",
    "clubstudioltd/craft-asset-rev": "^6.0",
    "topshelfcraft/environment-label": "^3.1",
    "craftcms/redactor": "2.1.7",
    "verbb/super-table": "^2.0",
    "angellco/spoon": "^3.2",
    "marionnewlevant/twig-perversion": "2.0.6",
    "verbb/field-manager": "2.0.4",
    "craftcms/aws-s3": "1.0.8",
    "rias/craft-colour-swatches": "^1.1",
    "nystudio107/craft-seomatic": "^3.1",
    "topshelfcraft/wordsmith": "^3.0",
    "viget/craft-video-embed": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
